<h2>All Tags</h2>

<div id="transaction table">
  <table style="width:100%">
    <tr>
      <th> Category</th>
    </tr>
  <% @tags.each do |tag| %>
   <tr>
     <td><%= tag.category %></td>
     <td> <a href="/tags/<%= tag.id %>/edit">Edit</a> </td>
     <td> <form action="/tags/<%= tag.id %>/delete" method="POST"><input type="submit" value="Delete"></form> </td>
    <% end %>

    </tr>

 </table>
